<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFF3F5F7"
    android:layout_height="match_parent">

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/v_fabulous_set_logo"
        android:layout_width="64dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@mipmap/fabulous_set_logo"
        android:layout_marginTop="40dp"
        android:layout_height="64dp"/>

    <androidx.cardview.widget.CardView
        android:id="@+id/v_fabulous_set_proxy"
        app:cardCornerRadius="12dp"
        app:cardElevation="0dp"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toBottomOf="@+id/v_fabulous_set_logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:cardBackgroundColor="@color/white"
        android:layout_width="match_parent"
        android:layout_height="49dp">

        <androidx.appcompat.widget.AppCompatTextView
            android:drawablePadding="11dp"
            android:textSize="14dp"
            android:textColor="#FF333333"
            android:gravity="center_vertical"
            android:layout_marginStart="20dp"
            android:layout_gravity="center_vertical"
            android:text="APP Proxy"
            android:drawableStart="@mipmap/fabulous_set_proxy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/v_fabulous_set_server"
        app:cardCornerRadius="12dp"
        app:cardElevation="0dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/v_fabulous_set_proxy"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:cardBackgroundColor="@color/white"
        android:layout_width="match_parent"
        android:layout_height="49dp">

        <androidx.appcompat.widget.AppCompatTextView
            android:drawablePadding="11dp"
            android:textSize="14dp"
            android:textColor="#FF333333"
            android:gravity="center_vertical"
            android:layout_marginStart="20dp"
            android:layout_gravity="center_vertical"
            android:text="Server List"
            android:drawableStart="@mipmap/fabulous_set_server"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/v_fabulous_set_share"
        app:cardCornerRadius="12dp"
        app:cardElevation="0dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/v_fabulous_set_server"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:cardBackgroundColor="@color/white"
        android:layout_width="match_parent"
        android:layout_height="49dp">

        <androidx.appcompat.widget.AppCompatTextView
            android:drawablePadding="11dp"
            android:textSize="14dp"
            android:textColor="#FF333333"
            android:gravity="center_vertical"
            android:layout_marginStart="20dp"
            android:layout_gravity="center_vertical"
            android:text="Share"
            android:drawableStart="@mipmap/fabulous_set_share"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/v_fabulous_set_policy"
        app:cardCornerRadius="12dp"
        app:cardElevation="0dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/v_fabulous_set_share"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:cardBackgroundColor="@color/white"
        android:layout_width="match_parent"
        android:layout_height="49dp">

        <androidx.appcompat.widget.AppCompatTextView
            android:drawablePadding="11dp"
            android:textSize="14dp"
            android:textColor="#FF333333"
            android:gravity="center_vertical"
            android:layout_marginStart="20dp"
            android:layout_gravity="center_vertical"
            android:text="Privacy Policy"
            android:drawableStart="@mipmap/fabulous_set_policy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        app:cardCornerRadius="20dp"
        app:cardElevation="0dp"
        app:cardBackgroundColor="#FF21D012"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginBottom="23dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.appcompat.widget.LinearLayoutCompat
            android:orientation="vertical"
            android:gravity="center_horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/v_fabulous_counting"
                android:textColor="@color/white"
                android:textSize="32dp"
                android:text="00:00:00"
                android:layout_marginTop="20dp"
                android:textStyle="bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <include
                android:layout_marginTop="16dp"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:layout_marginBottom="20dp"
                android:id="@+id/fabulous_set_speed"
                layout="@layout/fabulous_layout_speed"/>

        </androidx.appcompat.widget.LinearLayoutCompat>

    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>